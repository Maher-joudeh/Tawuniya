{
  "$schema": "https://raw.githubusercontent.com/microsoft/powerbi-desktop-samples/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.138.json",
  "name": "Tawuniya - General Theme (Light)",
  
  "dataColors": [
    "#6B47F5",
    "#312E52",
    "#8780F2",
    "#9073F2",
    "#A8A9CF",
    "#D6D1ED",
    "#5C57A2",
    "#7F67BC"
  ],
  
  "background": "#F5F7F9",
  "foreground": "#282639",
  "secondaryBackground": "#FFFFFF",
  "tableAccent": "#6B47F5",
  "firstLevelElements": "#282639",
  "secondLevelElements": "#62606F",
  "thirdLevelElements": "#E7E7E9",
  "fourthLevelElements": "#A2A1AA",
  
  "good": "#10B981",
  "bad": "#F43F5E",
  "neutral": "#777582",
  "maximum": "#6B47F5",
  "minimum": "#FFFBEB",
  "center": "#F59E0B",
  
  "textClasses": {
    "callout": {
      "fontSize": 45,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "title": {
      "fontSize": 12,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "header": {
      "fontSize": 12,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "label": {
      "fontSize": 10,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "largeLabel": {
      "fontSize": 12,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "smallLabel": {
      "fontSize": 9,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "lightLabel": {
      "fontSize": 10,
      "fontFace": "Tawuniya",
      "color": "#62606F"
    },
    "boldLabel": {
      "fontSize": 10,
      "fontFace": "Tawuniya",
      "color": "#282639",
      "fontWeight": "bold"
    },
    "largeLightLabel": {
      "fontSize": 12,
      "fontFace": "Tawuniya",
      "color": "#62606F"
    },
    "smallLightLabel": {
      "fontSize": 9,
      "fontFace": "Tawuniya",
      "color": "#62606F"
    },
    "largeTitle": {
      "fontSize": 14,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "semiboldLabel": {
      "fontSize": 10,
      "fontFace": "Tawuniya",
      "color": "#282639",
      "fontWeight": "bold"
    }
  },
  
  "visualStyles": {
    "*": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "transparency": 0
          }
        ],
        "border": [
          {
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 2,
                      "Percent": -0.5
                    }
                  }
                }
              }
            },
            "show": true,
            "width": 0.5
          }
        ],
        "categoryAxis": [
          {
            "showAxisTitle": false,
            "fontFamily": "Tawuniya",
            "fontSize": 10
          }
        ],
        "dropShadow": [
          {
            "show": false,
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 1,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "shadowBlur": 8,
            "angle": 45,
            "shadowDistance": 7,
            "transparency": 70,
            "position": "Outer"
          }
        ],
        "legend": [
          {
            "position": "TopCenter",
            "showTitle": false,
            "fontSize": 10,
            "fontFamily": "Tawuniya"
          }
        ],
        "padding": [
          {
            "top": 12,
            "bottom": 12,
            "left": 12,
            "right": 12
          }
        ],
        "spacing": [
          {
            "customizeSpacing": true,
            "spaceBelowTitle": 4
          }
        ],
        "subTitle": [
          {
            "show": false,
            "fontSize": 11,
            "fontFamily": "Tawuniya"
          }
        ],
        "title": [
          {
            "fontSize": 14,
            "bold": true,
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 1,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ],
        "valueAxis": [
          {
            "gridlineShow": true,
            "gridlineColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 2,
                      "Percent": -0.3
                    }
                  }
                }
              }
            },
            "gridlineStyle": "dotted",
            "gridlineThickness": 1,
            "showAxisTitle": false,
            "fontFamily": "Tawuniya",
            "fontSize": 10
          }
        ]
      }
    },
    "actionButton": {
      "*": {
        "fill": [
          {
            "$id": "default",
            "show": true,
            "fillColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "transparency": 90
          },
          {
            "$id": "hover",
            "show": true,
            "fillColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "transparency": 80
          },
          {
            "$id": "selected",
            "show": true,
            "fillColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "transparency": 75
          }
        ],
        "outline": [
          {
            "$id": "default",
            "show": true,
            "lineColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "weight": 0.5
          }
        ],
        "text": [
          {
            "$id": "default",
            "fontSize": 10,
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 1,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ]
      }
    },
    "bookmarkNavigator": {
      "*": {
        "fill": [
          {
            "$id": "default",
            "fillColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "transparency": 0
          },
          {
            "$id": "selected",
            "fillColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "transparency": 85
          }
        ],
        "outline": [
          {
            "$id": "default",
            "show": true,
            "lineColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 2,
                      "Percent": -0.4
                    }
                  }
                }
              }
            },
            "weight": 0.5
          }
        ],
        "text": [
          {
            "$id": "default",
            "fontSize": 10,
            "fontFamily": "Tawuniya"
          },
          {
            "$id": "selected",
            "fontSize": 10,
            "fontFamily": "Tawuniya",
            "bold": true
          }
        ],
        "shape": [
          {
            "tileShape": "rectangleRounded",
            "rectangleRoundedCurve": 6
          }
        ]
      }
    },
    "card": {
      "*": {
        "labels": [
          {
            "fontSize": 35,
            "bold": true,
            "fontFamily": "Tawuniya"
          }
        ]
      }
    },
    "cardVisual": {
      "*": {
        "accentBar": [
          {
            "$id": "default",
            "show": true,
            "width": 4,
            "position": "Top",
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ],
        "divider": [
          {
            "$id": "default",
            "show": true,
            "dividerColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 2,
                      "Percent": -0.4
                    }
                  }
                }
              }
            }
          }
        ],
        "label": [
          {
            "$id": "default",
            "position": "aboveValue",
            "fontSize": 12,
            "fontFamily": "Tawuniya"
          }
        ],
        "outline": [
          {
            "$id": "default",
            "show": true,
            "lineColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 2,
                      "Percent": -0.4
                    }
                  }
                }
              }
            }
          }
        ],
        "referenceLabel": [
          {
            "$id": "default",
            "backgroundShow": true,
            "backgroundColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "backgroundTransparency": 92
          }
        ],
        "shapeCustomRectangle": [
          {
            "$id": "default",
            "tileShape": "rectangleRounded",
            "rectangleRoundedCurve": 6
          }
        ],
        "title": [
          {
            "fontSize": 12,
            "fontFamily": "Tawuniya",
            "bold": true
          }
        ],
        "value": [
          {
            "$id": "default",
            "fontSize": 32,
            "bold": true,
            "fontFamily": "Tawuniya"
          }
        ]
      }
    },
    "clusteredColumnChart": {
      "*": {
        "labels": [
          {
            "show": true,
            "bold": true,
            "fontSize": 9,
            "fontFamily": "Tawuniya"
          }
        ]
      }
    },
    "donutChart": {
      "*": {
        "labels": [
          {
            "labelStyle": "Both",
            "fontSize": 10,
            "fontFamily": "Tawuniya"
          }
        ]
      }
    },
    "page": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": "#F5F7F9"
              }
            },
            "transparency": 0
          }
        ],
        "outspacePane": [
          {
            "backgroundColor": {
              "solid": {
                "color": "#F5F7F9"
              }
            },
            "fontFamily": "Tawuniya",
            "foregroundColor": {
              "solid": {
                "color": "#282639"
              }
            }
          }
        ],
        "filterCard": [
          {
            "$id": "Applied",
            "backgroundColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "foregroundColor": {
              "solid": {
                "color": "#282639"
              }
            },
            "fontFamily": "Tawuniya",
            "inputBoxColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            }
          }
        ]
      }
    },
    "pieChart": {
      "*": {
        "labels": [
          {
            "labelStyle": "Both",
            "fontSize": 10,
            "fontFamily": "Tawuniya"
          }
        ]
      }
    },
    "pivotTable": {
      "*": {
        "grid": [
          {
            "outlineColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 2,
                      "Percent": -0.4
                    }
                  }
                }
              }
            },
            "outlineWeight": 1,
            "rowPadding": 5
          }
        ],
        "columnHeaders": [
          {
            "fontFamily": "Tawuniya",
            "bold": true
          }
        ],
        "values": [
          {
            "fontFamily": "Tawuniya"
          }
        ]
      }
    },
    "slicer": {
      "*": {
        "background": [
          {
            "show": false
          }
        ],
        "header": [
          {
            "show": true,
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 1,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ],
        "items": [
          {
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 1,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ],
        "slider": [
          {
            "show": true,
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ]
      }
    },
    "tableEx": {
      "*": {
        "columnHeaders": [
          {
            "backColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": -0.9
                    }
                  }
                }
              }
            },
            "alignment": "Center",
            "bold": true,
            "fontFamily": "Tawuniya"
          }
        ],
        "grid": [
          {
            "gridHorizontal": true,
            "rowPadding": 4,
            "outlineColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 2,
                      "Percent": -0.4
                    }
                  }
                }
              }
            },
            "outlineWeight": 0.5
          }
        ],
        "values": [
          {
            "fontFamily": "Tawuniya",
            "backColorSecondary": {
              "solid": {
                "color": "#FFFFFF"
              }
            }
          }
        ]
      }
    },
    "textbox": {
      "*": {
        "background": [
          {
            "show": false
          }
        ],
        "general": [
          {
            "fontFamily": "Tawuniya",
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 1,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ]
      }
    }
  }
}
