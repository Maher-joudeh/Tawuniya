{
  "$schema": "https://raw.githubusercontent.com/microsoft/powerbi-desktop-samples/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.138.json",
  "name": "Tawuniya - General (Light Mode)",
  
  "_comment": {
    "version": "1.0.0",
    "lastUpdated": "2025-11-10",
    "tokenSource": "Figma Tokens Studio - alias-tokens-light mode",
    "designer": "Maher Joudeh",
    "email": "maher.joudeh@pwc.com",
    "client": "Tawuniya",
    "note": "All colors reference ThemeDataColor ColorId for dynamic updates. Structural colors added to dataColors array for consistent referencing."
  },
  
  "_foundations": {
    "_dataColors_mapping": {
      "0": "Data Colors.category-1 → Color.TAWN.Light.01 → #6B47F5",
      "1": "Data Colors.category-2 → Color.TAWN.Light.02 → #312E52",
      "2": "Data Colors.category-3 → Color.TAWN.Light.03 → #8780F2",
      "3": "Data Colors.category-4 → Color.TAWN.Light.04 → #9073F2",
      "4": "Data Colors.category-5 → Color.TAWN.Light.05 → #A8A9CF",
      "5": "Data Colors.category-6 → Color.TAWN.Light.06 → #D6D1ED",
      "6": "Data Colors.category-7 → Color.TAWN.Light.07 → #5C57A2",
      "7": "Data Colors.category-8 → Color.TAWN.Light.08 → #7F67BC",
      "8": "Background.primary → Color.Neutral.white → #FFFFFF",
      "9": "Background.primary-alt → Color.Neutral.25 → #F5F7F9",
      "10": "Text.primary → Color.Neutral.Dark Navy → #161528",
      "11": "Text.secondary → Color.Neutral.700 → #4E4C5C",
      "12": "Border.primary → Color.Neutral.300 → #A2A1AA",
      "13": "Border.secondary → Color.Neutral.200 → #B9B7BF"
    },
    "_structural_mapping": {
      "background": "ColorId 9 → Background.primary-alt",
      "foreground": "ColorId 10 → Text.primary",
      "secondaryBackground": "ColorId 8 → Background.primary",
      "tableAccent": "ColorId 0 → Brand.500",
      "secondLevelElements": "ColorId 11 → Text.secondary",
      "thirdLevelElements": "ColorId 13 → Border.secondary",
      "fourthLevelElements": "ColorId 12 → Border.primary"
    },
    "_sentiment_mapping": {
      "good": "Text.success → Color.Success.700 → #047857",
      "bad": "Text.error → Color.Error.700 → #BE123C",
      "neutral": "Text.disabled → Color.Neutral.400 → #8C8A96",
      "maximum": "ColorId 0 → Brand.500 → #6B47F5",
      "minimum": "Background.warning → Color.Warning.50 → #FFFBEB",
      "center": "Text.warning → Color.Warning.700 → #B45309"
    }
  },
  
  "dataColors": [
    "#6B47F5",
    "#312E52",
