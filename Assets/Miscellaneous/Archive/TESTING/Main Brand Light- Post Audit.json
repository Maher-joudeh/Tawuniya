{
  "$schema": "https://raw.githubusercontent.com/microsoft/powerbi-desktop-samples/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.138.json",
  "name": "Tawuniya - General Theme (Light Mode)",
  
  "dataColors": [
    "#6B47F5",
    "#8780F2",
    "#9073F2",
    "#A8A9CF",
    "#D6D1ED",
    "#5C57A2",
    "#7F67BC",
    "#312E52"
  ],
  
  "background": "#F5F7F9",
  "foreground": "#282639",
  "secondaryBackground": "#FFFFFF",
  "tableAccent": "#6B47F5",
  "firstLevelElements": "#282639",
  "secondLevelElements": "#62606F",
  "thirdLevelElements": "#E7E7E9",
  "fourthLevelElements": "#A2A1AA",
  
  "good": "#10B981",
  "bad": "#F43F5E",
  "neutral": "#777582",
  "maximum": "#6B47F5",
  "minimum": "#FFFBEB",
  "center": "#F59E0B",
  
  "textClasses": {
    "callout": {
      "fontSize": 45,
      "color": "#282639",
      "fontFace": "Tawuniya"
    },
    "title": {
      "fontSize": 12,
      "color": "#282639",
      "fontFace": "Tawuniya"
    },
    "header": {
      "fontSize": 12,
      "color": "#282639",
      "fontFace": "Tawuniya",
      "fontWeight": "bold"
    },
    "label": {
      "fontSize": 10,
      "color": "#282639",
      "fontFace": "Tawuniya"
    }
  },
  
  "visualStyles": {
    "*": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "transparency": 0
          }
        ],
        "border": [
          {
            "color": {
              "solid": {
                "color": "#E7E7E9"
              }
            },
            "show": true,
            "width": 0.5
          }
        ],
        "divider": [
          {
            "show": false,
            "width": 1
          }
        ],
        "categoryAxis": [
          {
            "showAxisTitle": false
          }
        ],
        "dropShadow": [
          {
            "show": false,
            "color": {
              "solid": {
                "color": "#161528"
              }
            },
            "shadowBlur": 8,
            "angle": 45,
            "shadowDistance": 7,
            "shadowSpread": 0,
            "transparency": 70,
            "position": "Outer",
            "preset": "Custom"
          }
        ],
        "legend": [
          {
            "position": "TopCenter",
            "showTitle": false,
            "fontSize": 7
          }
        ],
        "padding": [
          {
            "top": 10,
            "bottom": 10,
            "left": 10,
            "right": 10
          }
        ],
        "spacing": [
          {
            "customizeSpacing": true,
            "spaceBelowTitle": 3.5
          }
        ],
        "subTitle": [
          {
            "show": false,
            "fontSize": 11
          }
        ],
        "title": [
          {
            "fontSize": 14,
            "bold": true
          }
        ],
        "totals": [
          {
            "show": true,
            "bold": true,
            "fontSize": 7
          }
        ],
        "valueAxis": [
          {
            "gridlineShow": true,
            "gridlineColor": {
              "solid": {
                "color": "#E7E7E9"
              }
            },
            "gridlineStyle": "dotted",
            "gridlineThickness": 1,
            "showAxisTitle": false
          }
        ]
      }
    },
    "actionButton": {
      "*": {
        "border": [
          {
            "show": true
          }
        ],
        "dropShadow": [
          {
            "show": false
          }
        ],
        "fill": [
          {
            "show": true,
            "$id": "selected",
            "fillColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 2,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "transparency": 75
          }
        ],
        "icon": [
          {
            "show": true,
            "placement": "custom",
            "$id": "hover",
            "iconSize": 20
          }
        ],
        "outline": [
          {
            "show": false
          }
        ],
        "padding": [
          {
            "bottom": 0,
            "left": 0,
            "right": 0,
            "top": 0
          }
        ],
        "shadow": [
          {
            "$id": "default",
            "show": false,
            "shadowPositionPreset": "custom",
            "shadowBlur": 0,
            "shadowDistance": 18,
            "angle": 90,
            "transparency": 82
          }
        ],
        "text": [
          {
            "show": true,
            "$id": "default",
            "text": "Button",
            "horizontalAlignment": "left",
            "verticalAlignment": "middle",
            "fontSize": 7,
            "leftMargin": 30
          }
        ]
      }
    },
    "advancedSlicerVisual": {
      "*": {
        "background": [
          {
            "show": false
          }
        ],
        "border": [
          {
            "radius": 6,
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "show": false
          }
        ],
        "layout": [
          {
            "style": "Cards",
            "columnCount": 5,
            "rowCount": 2
          }
        ],
        "outline": [
          {
            "$id": "default",
            "show": false
          }
        ],
        "image": [
          {
            "$id": "selection",
            "position": "Left",
            "imageAreaSize": 30,
            "saturation": 100
          }
        ],
        "dropShadow": [
          {
            "show": false
          }
        ],
        "shapeCustomRectangle": [
          {
            "$id": "default",
            "tileShape": "rectangleRounded",
            "rectangleRoundedCurve": 7
          }
        ],
        "title": [
          {
            "show": false
          }
        ],
        "value": [
          {
            "show": true,
            "fontFamily": "Tawuniya",
            "fontSize": 12,
            "fontColor": {
              "solid": {
                "color": "#282639"
              }
            }
          }
        ]
      }
    },
    "bookmarkNavigator": {
      "*": {
        "border": [
          {
            "show": false
          }
        ],
        "dropShadow": [
          {
            "$id": "default",
            "show": false
          }
        ],
        "fill": [
          {
            "$id": "selected",
            "fillColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "transparency": 0
          }
        ],
        "layout": [
          {
            "orientation": 2,
            "cellPadding": 1
          }
        ],
        "outline": [
          {
            "$id": "default",
            "show": false,
            "lineColor": {
              "solid": {
                "color": "#62606F"
              }
            },
            "weight": 0.5
          }
        ],
        "padding": [
          {
            "bottom": 0,
            "left": 0,
            "right": 0,
            "top": 0
          }
        ],
        "shape": [
          {
            "$id": "default",
            "rectangleRoundedCurve": 12,
            "tileShape": "rectangleRounded"
          }
        ],
        "text": [
          {
            "$id": "selected",
            "fontSize": 8,
            "fontColor": {
              "solid": {
                "color": "#282639"
              }
            }
          }
        ]
      }
    },
    "card": {
      "*": {
        "labels": [
          {
            "fontSize": 35,
            "bold": true
          }
        ]
      }
    },
    "cardVisual": {
      "*": {
        "accentBar": [
          {
            "$id": "default",
            "show": true,
            "width": 4,
            "position": "Top",
            "color": {
              "solid": {
                "color": "#6B47F5"
              }
            }
          }
        ],
        "background": [
          {
            "show": false
          }
        ],
        "border": [
          {
            "show": true
          }
        ],
        "divider": [
          {
            "$id": "default",
            "show": true,
            "dividerColor": {
              "solid": {
                "color": "#D0CFD4"
              }
            }
          }
        ],
        "image": [
          {
            "$id": "default",
            "show": true,
            "imageType": "imageUrl",
            "position": "Left",
            "padding": 10,
            "size": 32
          }
        ],
        "label": [
          {
            "$id": "default",
            "position": "aboveValue",
            "fontSize": 14,
            "fontFamily": "Tawuniya"
          }
        ],
        "layout": [
          {
            "calloutSize": 60,
            "orientation": 2,
            "style": "Cards",
            "maxTiles": 3,
            "cellPadding": 8
          }
        ],
        "outline": [
          {
            "$id": "default",
            "show": false,
            "transparency": 0,
            "lineColor": {
              "solid": {
                "color": "#6B47F5"
              }
            }
          }
        ],
        "padding": [
          {
            "$id": "default",
            "paddingSelection": "Custom",
            "topMargin": 12,
            "bottomMargin": 12,
            "rightMargin": 8,
            "leftMargin": 8,
            "left": 0,
            "top": 0,
            "right": 0,
            "bottom": 0
          }
        ],
        "referenceLabel": [
          {
            "$id": "default",
            "backgroundShow": true,
            "backgroundColor": {
              "solid": {
                "color": "#6B47F5"
              }
            },
            "backgroundTransparency": 92.5
          }
        ],
        "referenceLabelLayout": [
          {
            "$id": "default",
            "outerPadding": 3
          }
        ],
        "referenceLabelTitle": [
          {
            "$id": "default",
            "titleFontSize": 9,
            "titleTransparency": 10
          }
        ],
        "referenceLabelValue": [
          {
            "$id": "default",
            "valueBold": false,
            "valueFontSize": 9,
            "valueTransparency": 10
          }
        ],
        "referenceLabelDetail": [
          {
            "$id": "default",
            "detailFontSize": 9,
            "detailTransparency": 90,
            "show": true,
            "detailBackgroundColor": {
              "solid": {
                "color": "#6B47F5"
              }
            }
          }
        ],
        "shapeCustomRectangle": [
          {
            "$id": "default",
            "tileShape": "rectangleRounded",
            "rectangleRoundedCurve": 7
          }
        ],
        "spacing": [
          {
            "$id": "default",
            "verticalSpacing": 0
          }
        ],
        "subTitle": [
          {
            "show": false
          }
        ],
        "shadowCustom": [
          {
            "show": false,
            "color": {
              "solid": {
                "color": "#282639"
              }
            }
          }
        ],
        "title": [
          {
            "fontSize": 9,
            "background": {
              "solid": {
                "color": "#F5F7F9"
              }
            },
            "text": "Add visual title here",
            "fontFamily": "Tawuniya"
          }
        ],
        "value": [
          {
            "$id": "default",
            "fontSize": 32,
            "horizontalAlignment": "left",
            "bold": true,
            "fontFamily": "Tawuniya"
          }
        ]
      }
    },
    "clusteredColumnChart": {
      "*": {
        "labels": [
          {
            "show": true,
            "bold": true,
            "fontSize": 7
          }
        ]
      }
    },
    "columnChart": {
      "*": {
        "legend": [
          {
            "show": false
          }
        ]
      }
    },
    "donutChart": {
      "*": {
        "legend": [
          {
            "show": false
          }
        ],
        "labels": [
          {
            "labelStyle": "Both"
          }
        ]
      }
    },
    "filledMap": {
      "*": {
        "mapStyles": [
          {
            "mapTheme": "grayscale"
          }
        ]
      }
    },
    "image": {
      "*": {
        "background": [
          {
            "show": false
          }
        ],
        "border": [
          {
            "show": false
          }
        ],
        "dropShadow": [
          {
            "show": false
          }
        ]
      }
    },
    "page": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": "#F5F7F9"
              }
            },
            "transparency": 0
          }
        ],
        "outspacePane": [
          {
            "backgroundColor": {
              "solid": {
                "color": "#F5F7F9"
              }
            }
          }
        ],
        "filterCard": [
          {
            "$id": "Applied",
            "backgroundColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "foregroundColor": {
              "solid": {
                "color": "#282639"
              }
            },
            "inputBoxColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "transparency": 0
          }
        ]
      }
    },
    "pieChart": {
      "*": {
        "legend": [
          {
            "show": false
          }
        ],
        "labels": [
          {
            "labelStyle": "Both"
          }
        ]
      }
    },
    "pivotTable": {
      "*": {
        "grid": [
          {
            "outlineColor": {
              "solid": {
                "color": "#6B47F5"
              }
            },
            "outlineWeight": 2,
            "rowPadding": 5
          }
        ],
        "rowHeaders": [
          {
            "wordWrap": false
          }
        ],
        "stylePreset": [
          {
            "name": "Minimal"
          }
        ],
        "title": [
          {
            "show": true,
            "text": "Table title"
          }
        ]
      }
    },
    "scatterChart": {
      "*": {
        "legend": [
          {
            "show": true,
            "position": "Left",
            "fontFamily": "Arial",
            "fontSize": 8
          }
        ],
        "categoryAxis": [
          {
            "show": true,
            "fontSize": 8
          }
        ],
        "valueAxis": [
          {
            "show": true,
            "position": "Left",
            "fontSize": 8
          }
        ]
      }
    },
    "shape": {
      "*": {
        "background": [
          {
            "show": true
          }
        ],
        "fill": [
          {
            "show": false
          }
        ],
        "outline": [
          {
            "show": false
          }
        ]
      }
    },
    "slicer": {
      "*": {
        "background": [
          {
            "show": false
          }
        ],
        "dropShadow": [
          {
            "show": false
          }
        ],
        "header": [
          {
            "show": false
          }
        ],
        "items": [
          {
            "background": {
              "solid": {
                "color": "#F5F7F9"
              }
            }
          }
        ],
        "padding": [
          {
            "bottom": 0,
            "top": 0,
            "left": 0,
            "right": 0
          }
        ],
        "slider": [
          {
            "color": {
              "solid": {
                "color": "#6B47F5"
              }
            },
            "handleFillColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "handleBorderColor": {
              "solid": {
                "color": "#6B47F5"
              }
            },
            "show": true
          }
        ],
        "date": [
          {
            "fontFamily": "Tawuniya"
          }
        ]
      }
    },
    "stackedAreaChart": {
      "*": {
        "legend": [
          {
            "show": true,
            "position": "Left",
            "fontSize": 8
          }
        ],
        "categoryAxis": [
          {
            "show": true,
            "fontSize": 8
          }
        ],
        "valueAxis": [
          {
            "show": true,
            "position": "Left",
            "fontSize": 8
          }
        ]
      }
    },
    "tableEx": {
      "*": {
        "columnHeaders": [
          {
            "backColor": {
              "solid": {
                "color": "#F0EDFE"
              }
            },
            "autoSizeColumnWidth": false,
            "alignment": "Center",
            "bold": true,
            "fontFamily": "Tawuniya"
          }
        ],
        "grid": [
          {
            "gridHorizontal": true,
            "rowPadding": 3.5,
            "outlineColor": {
              "solid": {
                "color": "#6B47F5"
              }
            },
            "outlineStyle": 1,
            "outlineWeight": 0.5
          }
        ],
        "title": [
          {
            "show": true,
            "text": "Table title",
            "fontFamily": "Tawuniya"
          }
        ],
        "values": [
          {
            "backColorSecondary": {
              "solid": {
                "color": "#FFFFFF"
              }
            }
          }
        ]
      }
    },
    "textbox": {
      "*": {
        "background": [
          {
            "show": false
          }
        ],
        "border": [
          {
            "show": false
          }
        ],
        "dropShadow": [
          {
            "show": false
          }
        ],
        "padding": [
          {
            "bottom": 0,
            "left": 0,
            "right": 0,
            "top": 0
          }
        ]
      }
    },
    "waterfallChart": {
      "*": {
        "legend": [
          {
            "show": false
          }
        ]
      }
    },
    "gauge": {
      "*": {
        "calloutValue": [
          {
            "fontFamily": "Tawuniya"
          }
        ]
      }
    }
  }
}
