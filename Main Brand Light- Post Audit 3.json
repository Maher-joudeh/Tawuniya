  "textClasses": {
    "callout": {
      "fontSize": 45,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "title": {
      "fontSize": 12,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "header": {
      "fontSize": 12,
      "fontFace": "Tawuniya",
      "color": "#282639",
      "fontWeight": "bold"
