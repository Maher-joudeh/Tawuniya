  "visualStyles": {
    "*": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 9,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "transparency": 0,
            "_colorRef": "ColorId 9 | Alias: Background.primary | Option Token: Color.Neutral.white"
          }
        ],
        "border": [
          {
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 12,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "show": true,
            "width": 0.5,
            "_colorRef": "ColorId 12 | Alias: Border.tertiary | Option Token: Color.Neutral.100"
          }
        ],
        "categoryAxis": [
          {
            "showAxisTitle": false,
            "labelColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 14,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "fontFamily": "Tawuniya",
            "_colorRef": "ColorId 14 | Alias: Text.tertiary | Option Token: Color.Neutral.600"
          }
        ],
        "dropShadow": [
          {
            "show": false,
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": -0.7
                    }
                  }
                }
              }
            },
            "shadowBlur": 8,
