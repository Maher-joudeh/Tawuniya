{
  "$schema": "https://raw.githubusercontent.com/microsoft/powerbi-desktop-samples/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.138.json",
  "name": "Tawuniya - Modular Theme (Light Mode)",
  
  "dataColors": [
    "#6B47F5",
    "#AC8CFF",
    "#009DFF",
    "#76CAFF",
    "#00B383",
    "#5ED1B1",
    "#5C57A2",
    "#7F67BC",
    "#F5F7F9",
    "#FFFFFF",
    "#282639",
    "#D0CFD4",
    "#E7E7E9",
    "#D0CFD4",
    "#62606F",
    "#10B981",
    "#F43F5E",
    "#F59E0B",
    "#777582"
  ],
  
  "background": "#F5F7F9",
  "foreground": "#282639",
  "secondaryBackground": "#FFFFFF",
  "tableAccent": "#6B47F5",
  "firstLevelElements": "#282639",
  "secondLevelElements": "#62606F",
  "thirdLevelElements": "#E7E7E9",
  "fourthLevelElements": "#A2A1AA",
  
  "good": "#10B981",
  "bad": "#F43F5E",
  "neutral": "#777582",
  "maximum": "#6B47F5",
  "minimum": "#FFFBEB",
  "center": "#F59E0B",
  
  "textClasses": {
    "callout": {
      "fontSize": 45,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "title": {
      "fontSize": 12,
      "fontFace": "Tawuniya",
      "color": "#282639"
    },
    "header": {
      "fontSize": 12,
      "fontFace": "Tawuniya",
      "color": "#282639",
      "fontWeight": "bold"
    },
    "label": {
      "fontSize": 10,
      "fontFace": "Tawuniya",
      "color": "#282639"
    }
  },
  
 
  "visualStyles": {
    "*": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 9,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "transparency": 0,
            "_colorRef": "ColorId 9 = Visual Background (white)"
          }
        ],
        "border": [
          {
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 12,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "show": true,
            "width": 0.5,
            "_colorRef": "ColorId 12 = Borders (Neutral.50)"
          }
        ],
        "divider": [
          {
            "show": false,
            "width": 1
          }
        ],
        "categoryAxis": [
          {
            "showAxisTitle": false,
            "labelColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 14,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "fontFamily": "Tawuniya",
            "_colorRef": "ColorId 14 = Secondary text"
          }
        ],
        "dropShadow": [
          {
            "show": false,
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": -0.7
                    }
                  }
                }
              }
            },
            "shadowBlur": 8,
            "angle": 45,
            "shadowDistance": 7,
            "transparency": 70,
            "_colorRef": "ColorId 10 at -70% = Very dark shadow"
          }
        ],
        "legend": [
          {
            "position": "TopCenter",
            "showTitle": false,
            "fontSize": 7,
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 14,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "_colorRef": "ColorId 14 = Secondary text"
          }
        ],
        "padding": [
          {
            "top": 10,
            "bottom": 10,
            "left": 10,
            "right": 10
          }
        ],
        "spacing": [
          {
            "customizeSpacing": true,
            "spaceBelowTitle": 3.5
          }
        ],
        "subTitle": [
          {
            "show": false,
            "fontSize": 11,
            "fontFamily": "Tawuniya"
          }
        ],
        "title": [
          {
            "fontSize": 14,
            "bold": true,
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "_colorRef": "ColorId 10 = Primary text"
          }
        ],
        "totals": [
          {
            "show": true,
            "bold": true,
            "fontSize": 7,
            "fontFamily": "Tawuniya"
          }
        ],
        "valueAxis": [
          {
            "gridlineShow": true,
            "gridlineColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 13,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "gridlineStyle": "dotted",
            "gridlineThickness": 1,
            "showAxisTitle": false,
            "labelColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 14,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "fontFamily": "Tawuniya",
            "_colorRef": "ColorId 13 = Gridlines, ColorId 14 = Labels"
          }
        ]
      }
    },
    "actionButton": {
      "*": {
        "border": [
          {
            "show": true,
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 12,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ],
        "fill": [
          {
            "show": true,
            "$id": "default",
            "fillColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 9,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "transparency": 0
          },
          {
            "show": true,
            "$id": "selected",
            "fillColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "transparency": 75,
            "_colorRef": "ColorId 0 = Primary brand color at 25% opacity"
          }
        ],
        "text": [
          {
            "show": true,
            "$id": "default",
            "text": "Button",
            "fontSize": 7,
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ]
      }
    },
    "cardVisual": {
      "*": {
        "accentBar": [
          {
            "$id": "default",
            "show": true,
            "width": 4,
            "position": "Top",
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "_colorRef": "ColorId 0 = Primary brand color"
          }
        ],
        "border": [
          {
            "show": true,
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 12,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ],
        "divider": [
          {
            "$id": "default",
            "show": true,
            "dividerColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 13,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "_colorRef": "ColorId 13 = Divider color"
          }
        ],
        "label": [
          {
            "$id": "default",
            "position": "aboveValue",
            "fontSize": 14,
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 14,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "_colorRef": "ColorId 14 = Secondary text"
          }
        ],
        "referenceLabel": [
          {
            "$id": "default",
            "backgroundShow": true,
            "backgroundColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "backgroundTransparency": 92.5,
            "_colorRef": "ColorId 0 at 7.5% opacity"
          }
        ],
        "title": [
          {
            "fontSize": 9,
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "background": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 8,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "_colorRef": "ColorId 8 = Canvas background"
          }
        ],
        "value": [
          {
            "$id": "default",
            "fontSize": 32,
            "bold": true,
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ]
      }
    },
    "page": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 8,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "transparency": 0,
            "_colorRef": "ColorId 8 = Page canvas background"
          }
        ],
        "outspacePane": [
          {
            "backgroundColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 8,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "fontFamily": "Tawuniya",
            "foregroundColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ],
        "filterCard": [
          {
            "$id": "Applied",
            "backgroundColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 9,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "foregroundColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "fontFamily": "Tawuniya"
          }
        ]
      }
    },
    "slicer": {
      "*": {
        "items": [
          {
            "background": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 8,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ],
        "slider": [
          {
            "color": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "handleFillColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 9,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "handleBorderColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ]
      }
    },
    "tableEx": {
      "*": {
        "columnHeaders": [
          {
            "backColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 0,
                      "Percent": 0.8
                    }
                  }
                }
              }
            },
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "_colorRef": "ColorId 0 at 90% lightness (Brand.50)"
          }
        ],
        "grid": [
          {
            "gridHorizontal": true,
            "rowPadding": 3.5,
            "outlineColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 12,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ],
        "values": [
          {
            "backColorSecondary": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 9,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": 0
                    }
                  }
                }
              }
            }
          }
        ]
      }
    }
  }
}
