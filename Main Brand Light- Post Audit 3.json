          }
        ],
        "legend": [
          {
            "position": "TopCenter",
            "showTitle": false,
            "fontSize": 7,
            "fontFamily": "Tawuniya",
            "fontColor": {
              "solid": {
                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 14,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "_colorRef": "ColorId 14 | Alias: Text.tertiary | Option Token: Color.Neutral.600"
