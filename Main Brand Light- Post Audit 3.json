                "color": {
                  "expr": {
                    "ThemeDataColor": {
                      "ColorId": 10,
                      "Percent": 0
                    }
                  }
                }
              }
            },
            "_colorRef": "ColorId 10 | Alias: Text.primary | Option Token: Color.Neutral.900"
          }
        ]
